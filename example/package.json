  "version": "1.0.0",
  "description": "template of redux web projects",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build:clean": "rimraf ./dist/*",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --color -p",
